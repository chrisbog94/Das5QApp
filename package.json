{
  "name": "das-keyboard-q",
  "description": "Version 2 - Das Keyboard Q application",
  "version": "3.2.4",
  "main": "electron.js",
  "homepage": "http://daskeyboard.com",
  "license": "MIT",
  "scripts": {
    "node-gyp": "node-gyp --version",
    "ng": "ng",
    "start": "export Q_NODE_ENV=development && concurrently \"ng serve\" \"yarn electron-dev\" ",
    "start-win": "echo \"export manually Q_NODE_ENV=development\" && concurrently \"ng serve\" \"yarn electron-dev\" ",
    "electron-dev": "yarn build-electron-dev && electron dist",
    "start-prod": "export Q_NODE_ENV=production && yarn electron",
    "dist": "electron dist",
    "build": "ng build --prod",
    "test": "ng lint && ng test --configuration=singleRun && yarn e2e",
    "test-travis": "ng lint &&  ng test --watch=false && yarn e2e",
    "test-public-api": "cd scripts/public-api-test/ && ./run-api-test.sh",
    "test-no-available-port": "cd src/electron/testing-scripts && node test-fatal-error-no-available-port.ts",
    "test-colorize": "cd scripts/public-api && ./colorize-5Q.sh",
    "unit": "mocha",
    "lint": "ng lint",
    "electron-dist": "electron dist",
    "component": "ng generate component",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e --configuration=test",
    "protractor": "protractor",
    "build-electron": "rm -rf dist && ng build --prod --base-href ./ && cp package.json dist && yarn grunt && cp src/electron/*.html dist/",
    "build-electron-dev": "rm -rf dist && ng build --base-href ./ && cp -R src/electron/* dist && cp package.json dist",
    "run-electron": "cd src/electron-dev && electron .",
    "electron": "yarn build-electron && electron dist",
    "executable-linux": "cd dist && yarn && cd .. && yarn package-linux",
    "executable-win-x64": "rm -rf das-keyboard-q-win32-x64 && cd dist && yarn && cd .. && yarn package-win-x64",
    "executable-win-ia32": "rm -rf das-keyboard-q-win32-ia32 && cd dist && yarn && cd .. && yarn package-win-ia32",
    "executable-mac": "cd dist && yarn  && cd .. && yarn package-mac",
    "grunt": "grunt uglify:t1 && grunt obfuscator:task1 && cp src/electron/lib/logging.json dist/lib/logging.json ",
    "package-mac": "electron-packager ./dist \"Das Keyboard Q\" --overwrite --platform=darwin --arch=x64 --icon=icon.icns",
    "package-win-ia32": "electron-packager ./dist das-keyboard-q --overwrite --platform=win32  --arch=ia32 --icon=icon.ico --asar.unpackDir=\"{**/assets/images,assets/scripts,assets/externalLibraries}\"",
    "package-win-x64": "electron-packager ./dist das-keyboard-q --overwrite --platform=win32  --arch=x64 --icon=icon.ico --asar.unpackDir=\"{**/assets/images,assets/scripts,assets/externalLibraries}\"",
    "package-linux": "electron-packager ./dist das-keyboard-q --overwrite --platform=linux  --arch=x64 --prune=true  --icon=q-icon.png --asar.unpackDir=\"{**/assets/images,assets/scripts,assets/externalLibraries}\"",
    "asar-linux": "asar pack q-Desktop-linux-x64/resources/app q-Desktop-linux-x64/resources/app.asar && cd q-Desktop-linux-x64/resources && rm -rf app && cd ..",
    "asar-win": "asar pack q-Desktop-win32-ia32/resources/app q-Desktop-win32-ia32/resources/app.asar && cd q-Desktop-win32-ia32/resources && rm -rf app && cd .."
  },
  "private": true,
